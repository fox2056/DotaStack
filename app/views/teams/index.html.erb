<div class="container">

  <!-- Main component for a primary marketing message or call to action -->
  <% @teams.each do |team| %>
  <div class="jumbotron">
    <h1><%= team.title %></h1>
    <img class="img-circle" src="<%= team.logo %>" alt="Generic placeholder image" width="140" height="140">
    <p><%= team.rating %></p>
    <p><%= team.body %></p>
    <p>
      <%= link_to 'Show', team, class: "btn btn-lg btn-primary"%>
      <%= link_to 'Edit', edit_team_path(team), class: "btn btn-lg btn-primary"%>
      <%= link_to 'Destroy', team, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-lg btn-primary"%>
    </p>
  </div>
  <% end %>

</div>
<br>


<footer class="footer">
  <div class="container">
    <p class="text-muted">Добавь свою команду.<%= link_to 'New Team', new_team_path, class: "btn btn-lg btn-primary"%></p>
